{
	"plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
	"printWidth": 120,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "es5",
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		},
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		},
		{
			"files": ["*.ts"],
			"options": {
				"parser": "typescript"
			}
		}
	]
}
